{% extends 'base.html.twig' %}
{% block page_title %}
    Cr√©ation d'utilisateur
{% endblock page_title %}

{% block page_subtitle '' %}

{% block page_content %}
    {% form_theme score_form '@AdminLTE/layout/form-theme-horizontal.html.twig' %}
    <div class="box">
        <div class="box-header with-border">
            <div class="box-tools pull-right">
            </div>
            <!-- /.box-tools -->
        </div>
        <!-- /.box-header -->
        <div class="box-body">
            <div class="container">
                <div class="row">
                    <div class="col-lg-12">
                        {% for message in app.flashes('success') %}
                            <div class="alert alert-success">
                                {{ message }}
                            </div>
                        {% endfor %}
                        {{ form_start(score_form) }}
                        {{ form_widget(score_form) }}
                        {{ form_end(score_form) }}
                    </div>
                </div>
            </div>
        </div>
        <!-- /.box-body -->
        <div class="box-footer">
        </div>
        <!-- box-footer -->
    </div>
    <!-- /.box -->
{% endblock page_content %}